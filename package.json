{
  "name": "library",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "swc src --out-dir build/src -w --sync",
    "watch": "nodemon --watch './build/src/**/*' -e js build/src/index.js",
    "copy": "cp -r prisma/generated build/prisma",
    "copy-env": "cp -r .env build",
    "dev": "concurrently 'pnpm copy' 'pnpm copy-env' 'pnpm build' 'pnpm watch'",
    "start": "pnpm build && cd build/src && node index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.174",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.30",
    "chokidar": "^3.5.3",
    "concurrently": "^7.1.0",
    "nodemon": "^2.0.16",
    "prisma": "^3.13.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "body-parser": "^1.20.0",
    "express": "^4.18.1",
    "morgan": "^1.10.0"
  }
}
